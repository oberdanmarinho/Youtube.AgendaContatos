@model List<ContatoModel>

<table id="table-contatos-usuario" class="display" style="width:100%">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">Nome</th>
			<th scope="col">E-mail</th>
			<th scope="col">Telefone</th>
		</tr>
	</thead>
	<tbody>
		@if (Model != null && Model.Any())
		{
			foreach (var contato in Model)
			{
				<tr>
					<th scope="row">@contato.Id</th>
					<td>@contato.Nome</td>
					<td>@contato.Email</td>
					<td>@contato.Telefone</td>
				</tr>
			}
		}
		else
		{
			<tr class="add"><td valign="top" colspan="4">Nenhum registro encontrado.</td></tr>
		}
	</tbody>
</table>

